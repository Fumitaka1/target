<div class="wrapper flex top">
  <div class="home-outer">
    <%= will_paginate @users, previous_label: '<', next_label: '>', inner_window: 1 %>
    <% @users.each do |user| %>
    <div class="post">
      <div class="post_info">
        <%= link_to image_tag("icon.png", alt: "user icon", class: "user-icon"), "" %>
        <div class="post_info_item">
          <div class="post_info_nav">
            <p class="user_name"><%= link_to user.name, users_path(user) %></p>
          </div>
        </div>
      </div>
    </div>
    <% end %>
    <%= will_paginate @users, previous_label: '<', next_label: '>', inner_window: 1 %>
  </div>
  <div class="sidemenu">
    <%= render 'users/users_side' %>
  </div>
</div>
